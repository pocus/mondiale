<div class="my_trip">
    <%= image_tag t.trip_cover, class: "trip-cover" %>
    <div class="trip-summary">
      <h1 class="title"><%= link_to t.title, t, class: "link" %></h1>
      <div class="date"><%= trip_date(t) %></div>
    </div>
    <div class="cover-map" data-objects="<%= posts_in_json(t.posts) %>"></div>
</div>